<div class="formulario">
  <%= simple_form_for @agente do |f| %>
    <%= f.input :primerNombre, input_html: { autocomplete: "off", autofocus: "on" } %>
    <%= f.input :segundoNombre, input_html: { autocomplete: "off" } %>
   	<%= f.input :primerApellido, input_html: { autocomplete: "off" } %>
   	<%= f.input :nombreUds, input_html: { autocomplete: "off" } %>
   	<%= f.input :modalidad, input_html: { autocomplete: "off" } %>
   	<%= f.input :correo, input_html: { autocomplete: "off" } %>
    <%= f.button :submit %>
  <% end %>
</div>
