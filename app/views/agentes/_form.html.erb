<%= simple_form_for @agente, wrapper: :floating_labels_form do |f| %>
  <%= f.input :primerNombre %>
  <%= f.input :segundoNombre %>
  <%= f.input :primerApellido %>
  <%= f.input :nombreUds %>
  <%= f.input :modalidad %>
  <%= f.input :correo %>
  <%= f.input :cedula %>
  <div class="botones">
  	<%= f.button :submit, class: "btn btn-info" %>
  	<div class="btn btn-secondary">
  		<i class="fas fa-undo-alt"></i> <%= link_to 'Regresar', agentes_path %>
  	</div>
  </div>
<% end %>